{
  extends: ["config:base"],
  automerge: true,
  regexManagers: [
    {
      fileMatch: ["aqua-installer"],
      matchStrings: ["version=(?<currentValue>.*?) # renovate: depName=aquaproj/aqua\\n"],
      depNameTemplate: "aquaproj/aqua",
      datasourceTemplate: "github-releases",
    },
    {
      fileMatch: ["README.md"],
      depNameTemplate: "aquaproj/aqua-installer",
      datasourceTemplate: "github-releases",
      matchStrings: [
        "aqua-installer/(?<currentValue>.*)/aqua-installer",
        "aqua-installer@(?<currentValue>.*)",
      ],
    },
    {
      fileMatch: ["README.md"],
      depNameTemplate: "aquaproj/aqua",
      datasourceTemplate: "github-releases",
      matchStrings: [
        "aqua_version: (?<currentValue>.*?)\\n",
        "bash -s -- -v (?<currentValue>.*) -i",
      ],
    },
  ],
}
